    <TfrxADOQuery Name="Clin_RecDetail" UserName="Clin_RecDetail" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" Master="Clin_RecMaster" IgnoreDupParams="False" SQL.Text="Select case when (GroupNum/10000) = 99 then (case when a.DrugUse='' then 0 else RecDetailID end) else GroupNum/10000 end as GroupID, &#13;&#10;  a.*, (c.BaseSign &#38; 0x02) as SkinTest, d.InjectSign,&#13;&#10;  case when  d.InjectSign = 1 and a.DayCount &#62; 0 then '×'+cast(DayCount as varchar(8)) + '天' else '' end as DayCount2,&#13;&#10;  (select FrequencyName from His_Base.dbo.D_Frequency b (nolock) where a.Frequency = b.Frequency) as FrequencyName,&#13;&#10;  case when d.InjectSign = 1 then '' else a.Specs end as Specs2,&#13;&#10;  case when d.InjectSign = 1 then '' else cast(cast(Quan as float) as varchar(12)) end as Quan2,&#13;&#10;  case when d.InjectSign = 1 then '' else a.CUnit end as CUnit2,&#13;&#10;  case when d.InjectSign = 1 then cast(cast(Dosage as float) as varchar(12)) else '' end as Dosage2,&#13;&#10;  case when d.InjectSign = 1 then a.DoseUnit else '' end as DoseUnit2,&#13;&#10;  case when d.InjectSign = 1 then '' else '每次' + cast(cast(Dosage as float) as varchar(12)) end as Dosage3,&#13;&#10;  case when d.InjectSign = 1 then '' else a.DoseUnit end as DoseUnit3                       &#13;&#10;FROM Clin_RecDetail a (nolock)      &#13;&#10;  left join His_Base.dbo.Base_FeeInfo c (nolock) on a.FeeID = c.FeeID&#13;&#10;  left join His_Base.dbo.D_DrugUse d (nolock) on a.DrugUse = d.DrugUse                                                                                                                                                   &#13;&#10;where RecipeID =:RecipeID&#13;&#10;order by GroupNum, RecDetailID                                                   " CommandTimeout="30" Database="ReportDB" LockType="ltReadOnly" PropData="05704C6566740394000470546F7003CC000A506172616D657465727301010625204E616D653D225265636970654944222044617461547970653D226674496E746567657222