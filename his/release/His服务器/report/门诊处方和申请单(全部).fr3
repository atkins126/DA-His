 DripSpeed: Int;&#13;&#10;  DripSpeedText: string;&#13;&#10;  RecExtendText: string;&#13;&#10;&#13;&#10;procedure DetailData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if not SkinTest then&#13;&#10;    SkinTest := &#60;Clin_RecDetail.&#34;SkinTest&#34;&#62;;&#13;&#10;  if (DripSpeed &#60;= 0) and (&#60;Clin_RecDetail.&#34;DripSpeed&#34;&#62; &#62; 0)&#13;&#10;    and (&#60;Clin_RecDetail.&#34;InjectSign&#34;&#62;)  then&#13;&#10;    DripSpeed := &#60;Clin_RecDetail.&#34;DripSpeed&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SkinTest := False;&#13;&#10;  DripSpeed := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupFooter1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SkinTestMemo.Visible := SkinTest;&#13;&#10;&#13;&#10;  if (DripSpeed &#62; 0) then&#13;&#10;    DripSpeedText := '滴速:' + IntToStr(DripSpeed) + '滴/分钟'&#13;&#10;  else&#13;&#10;    DripSpeedText := '';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  RecExtendText := '';&#13;&#10;  if (&#60;Clin_RecMaster.&#34;ExtendSign&#34;&#62; and $02) &#60;&#62; 0 then&#13;&#10;    RecExtendText := '麻醉'&#13;&#10;  else if (&#60;Clin_RecMaster.&#34;ExtendSign&#34;&#62; and $04) &#60;&#62; 0 then&#13;&#10;    RecExtendText := '二类精神';&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  ReportDB.Connected := False;&#13;&#10;  ReportDB.DatabaseName := DB_His_Data;&#13;&#10;  Page1.Visible := not Clin_RecMaster.Eof;&#13;&#10;  Page2.Visible := not Clin_RecMaster_RecType.Eof;&#13;&#10;&#13;&#10;   // 重症打印两份&#13;&#10;end." StoreInDFM="False">
