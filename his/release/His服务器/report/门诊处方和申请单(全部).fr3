    <TfrxADOQuery Name="Clin_RecMaster" UserName="Clin_RecMaster" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select a.*, c.OfficeAddress,                                                                 &#13;&#10;case when (a.ExtendSign &#38; 1) &#60;&#62; 0 then '重症' &#13;&#10;  when (a.ExtendSign &#38; 8) &#60;&#62; 0 then '工伤' &#13;&#10;  else '' &#13;&#10;end as ExtendText1, '' as Extend1Text,          &#13;&#10;(select sum(SumPrice) from Clin_RecDetail c (nolock) where c.RecipeID = a.RecipeID) as SumPrice&#13;&#10;from Clin_RecMaster a (nolock) inner join (select * from Clin_RecMaster where RecipeID=:RecipeID) b&#13;&#10;       on a.ClinID=b.ClinID and a.ClinFeeID = b.ClinFeeID and a.DoctorID = b.DoctorID                                                              &#13;&#10;    left join His_Sys.dbo.Sys_Office c (nolock) on c.OfficeID = a.ExecOfficeID                                                                                                                                             &#13;&#10;where a.RecTypeID &#60; '20' and dbo.Fun_Clin_Recipe_CanPrint(a.RecTypeID, a.CheckSign) = 1                                                                                                                              &#13;&#10;order by a.RecipeID    &#13;&#10;    " CommandTimeout="30" Database="ReportDB" LockType="ltReadOnly" PropData="05704C6566740397000470546F700388000A506172616D657465727301010645204E616D653D225265636970654944222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B5265636970654944262336323B220000"/>
